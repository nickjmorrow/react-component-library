  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^10.14.8",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.19",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "20.0.3",
